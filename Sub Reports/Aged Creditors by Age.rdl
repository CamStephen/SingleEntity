          <DataField>DOC_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Age91120">
          <Value>=cdec(iif(Fields!Debt_Age.Value&gt;90,iif(Fields!Debt_Age.Value&lt;=120,Fields!VALUE.Value,0),0))</Value>
        </Field>
