          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SORT">
          <DataField>SORT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DOCDATE">
          <DataField>DOCDATE</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
