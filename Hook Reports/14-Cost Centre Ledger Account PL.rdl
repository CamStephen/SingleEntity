  <Code>Public Function Divide(Var as double, Bud as double) AS double
    IF IsNothing(Var) Or IsNothing(Bud) Or Var = 0.00 Then
        Divide = 0
    ELSEIF Bud &lt;&gt; 0.00
        Divide = Var  / Bud
    ELSE
        Divide = 0
    END IF
End Function</Code>
