          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ACTUAL_INCOME">
          <Value>=IIF(Fields!MAINGROUP.Value = "1",Fields!ACTUALS.Value,CDEC(0))</Value>
        </Field>
        <Field Name="ACTUAL_COST">
          <Value>=IIF(Fields!MAINGROUP.Value = "2",Fields!ACTUALS.Value,CDEC(0))</Value>
