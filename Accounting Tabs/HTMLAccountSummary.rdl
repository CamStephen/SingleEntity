    <DataSet Name="dsGetDetails">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@YEARCODE">
            <Value>=Parameters!YEARCODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@COMPANYCODE">
            <Value>=Parameters!COMPANYCODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@BUDGET">
            <Value>=Parameters!BUDGET.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@LEDGER">
            <Value>=Parameters!LEDGER.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DOMAINNAME">
            <Value>=Parameters!UserID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ADD_PERIDOS">
            <Value>=JOIN(Parameters!ADDPERIODS.Value,",")</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>exec PSF_RPT_SEC_GET_ACCOUNT_SUMMARY_DETAILS
@YEARCODE, @COMPANYCODE, @BUDGET, @LEDGER, @DOMAINNAME, @ADD_PERIDOS</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="NOMINAL">
          <DataField>NOMINAL</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LEFTTOSPEND">
          <Value>=Fields!BUDGET.Value-Fields!ACTUAL.Value-Fields!COMMITMENTS.Value</Value>
        </Field>
        <Field Name="ACCOUNT">
          <DataField>ACCOUNT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ACTUAL">
          <Value>=CDEC(IIF(FIELDS!STATUS.Value = "ACTUAL", FIELDS!VALUE.Value, 0))</Value>
        </Field>
        <Field Name="COMMITMENTS">
          <Value>=CDEC(IIF(FIELDS!STATUS.Value = "COMMITMENTS", 

	IIF(Parameters!COMMITMENTS.Value = "YTD"
	,
		IIF(Left(FIELDS!YEARPERIOD.Value, InStr(FIELDS!YEARPERIOD.Value,".")-1) &lt;= Parameters!YEARCODE.Value ,FIELDS!VALUE.Value, CDEC(0.00))
		,
			IIF(Parameters!COMMITMENTS.Value = "CURRYEAR"
			,
				IIF(Left(FIELDS!YEARPERIOD.Value, InStr(FIELDS!YEARPERIOD.Value,".")-1) = Parameters!YEARCODE.Value,FIELDS!VALUE.Value, CDEC(0.00))
			,
			IIF(Parameters!COMMITMENTS.Value = "ALL",FIELDS!VALUE.Value, CDEC(0.00))
			)
		)

, 0))</Value>
        </Field>
        <Field Name="SUPPLIER">
          <DataField>SUPPLIER</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BUDGET">
          <Value>=CDEC(IIF(FIELDS!STATUS.Value = Parameters!BUDGET.Value, FIELDS!VALUE.Value, 0))</Value>
        </Field>
        <Field Name="YEARCODE">
          <DataField>YEARCODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PERIOD">
          <DataField>PERIOD</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BUDGET_REMAINING">
          <Value>=FIELDS!BUDGET.Value - (FIELDS!ACTUAL.Value + FIELDS!COMMITMENTS.Value)</Value>
        </Field>
        <Field Name="VALUE">
          <DataField>VALUE</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="COMPANYCODE">
          <DataField>COMPANYCODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BUDGETCODE">
          <DataField>BUDGETCODE</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="STATUS">
