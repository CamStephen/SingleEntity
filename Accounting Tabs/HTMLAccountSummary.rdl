  <Code>Public Shared Function SetColor(ByVal Value As Integer) As String
SetColor = "Black"
If Value &lt; 0.00 Then 
SetColor = "Red"
End If
End Function

Public Function ReplaceNull(VarValue as Double) AS Double

IF (IsNothing(VarValue)) 

                ReplaceNull = 0
ELSE 

                ReplaceNull = VarValue

END IF

End Function</Code>
