    <DataSet Name="dsGetCurrentYearSpend">
      <Query>
        <DataSourceName>dsselectedDatabase</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@YEARCODE">
            <Value>=Parameters!YEARCODE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SUPPLIER">
            <Value>=Parameters!SUPPLIER.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@XLEDGER">
            <Value>=Parameters!XLEDGER.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@COMPANYCODE">
            <Value>=Parameters!COMPANYCODE.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH CTE AS
(
	SELECT PERIOD, YEARCODE, MA.ACCOUNT, MA.LEDGER
	FROM M_PERIOD
		CROSS JOIN M_ACCOUNT MA
	WHERE YEARCODE = @YEARCODE
			AND PERIOD NOT IN ('98','99','REG','00')
			AND MA.ACCOUNT = @SUPPLIER
			AND MA.LEDGER = @XLEDGER
			and (COMPANYCODE = @COMPANYCODE or COMPANYCODE is NULL)
)
,CTE2 AS
(

	SELECT XACCOUNT,XLEDGER,ACCOUNT,LEDGER, ISNULL(SUM(DD.VALUE),0) VALUE, YEARCODE, PERIOD
	FROM D_DETAILS DD
	WHERE DD.TRANSACTIONTYPE=0 
	AND DD.COMPANYCODE=@COMPANYCODE
	AND ( (XACCOUNT = @SUPPLIER AND XLEDGER = @XLEDGER )
			AND (isnull(ACCOUNT,'none') &lt;&gt; @SUPPLIER AND isnull(LEDGER,'none') &lt;&gt; @XLEDGER)
		)
	and (isnull(LEDGER,'none') not in (SELECT ml.LEDGER from M_LEDGER ML where TYPE = 'x'))
	AND DD.YEARCODE = @YEARCODE

	GROUP BY XACCOUNT,ACCOUNT,LEDGER,XLEDGER, YEARCODE, PERIOD

)
,CTE3 AS
(
	SELECT  DP.ACCOUNT,
		SUM(MC.COMMITVAL) VALUE,
		MC.YEARCODE,
		MC.PERIOD, 
		DP.NOMINAL,
		DP.COMPANYCODE
FROM M_COMMITMENTS MC
		INNER JOIN DP_DETAILS DP ON DP.ID = MC.DETAIL_ID
		INNER JOIN M_NOMINAL MN ON MN.COMPANYCODE = DP.COMPANYCODE AND MN.NOMINAL = DP.NOMINAL
WHERE COMMITVAL &lt;&gt; 0 
			AND MC.COMPANYCODE=@COMPANYCODE
			AND DP.ACCOUNT = MN.NOMAN1 + '_'+ @SUPPLIER
GROUP BY DP.ACCOUNT,MC.YEARCODE,MC.PERIOD, 
		DP.NOMINAL,
		DP.COMPANYCODE

)


SELECT C1.YEARCODE, C1.PERIOD, C1.ACCOUNT, C1.LEDGER, ISNULL(C2.VALUE,0) VALUE , 'Actual' AS STATUS
FROM CTE C1
	LEFT OUTER JOIN CTE2 C2 ON C1.YEARCODE = C2.YEARCODE AND C1.PERIOD = C2.PERIOD
UNION ALL 
SELECT C1.YEARCODE, C1.PERIOD, C1.ACCOUNT, C1.LEDGER, ISNULL(C3.VALUE,0) VALUE , 'Authorised Commitments'
FROM CTE C1
	LEFT OUTER JOIN CTE3 C3 ON C1.YEARCODE = C3.YEARCODE AND C1.PERIOD = C3.PERIOD</CommandText>
      </Query>
